int main() {
    int x = 1;

    if (x > 0)
        print("Powinno tu wejsc");

    if (x > 10)
        print("Nie powinno tu wejsc");
    else
        print("A tu powinno wejsc");

    while (x < 10) {
        print(x);
        x = x + 1;
    }

    for i = 1 to 10 do
        print("for, i = " + toString(i));

    for i = 10 downto 1 do
        print("I w druga strone, i = " + toString(i));
}
